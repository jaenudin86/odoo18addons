<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_customer_code" model="ir.sequence">
        <field name="name">Customer Code Sequence</field>
        <field name="code">customer.code.sequence</field>
        <field name="padding">7</field>
        <field name="number_next_actual">1</field>
        <field name="number_increment">1</field>
        <field name="implementation">standard</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_supplier_code" model="ir.sequence">
        <field name="name">Supplier Code Sequence</field>
        <field name="code">supplier.code.sequence</field>
        <field name="padding">4</field>
        <field name="number_next_actual">1</field>
        <field name="number_increment">1</field>
        <field name="implementation">standard</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
